{
  "name": "rf-template",
  "version": "0.0.1",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "test": "react-scripts test --env=jsdom"
  },
  "betterScripts": {
    "start": {
      "command": "rf-scripts start",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "rf-scripts build",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4",
    "rf-scripts": "latest"
  },
  "devDependencies": {
    "better-npm-run": "^0.1.0"
  },
  "license": "MIT"
}
