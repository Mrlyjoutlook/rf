{
  "name": "redux",
  "version": "0.1.0",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "dll": "better-npm-run dll"
  },
  "betterScripts": {
    "start": {
      "command": "rf-scripts start",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "rf-scripts build",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "dll": {
      "command": "rf-scripts dll",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4",
    "rf-scripts": "latest",
    "redux": "^4.0.0",
    "react-redux": "^5.0.7",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "better-npm-run": "^0.1.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0"
  },
  "license": "MIT",
  "private": true
}