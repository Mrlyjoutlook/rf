{
  "name": "mobx",
  "version": "0.1.0",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "dll": "better-npm-run dll"
  },
  "betterScripts": {
    "start": {
      "command": "rf-scripts start",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "rf-scripts build",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "dll": {
      "command": "rf-scripts dll",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4",
    "rf-scripts": "latest",
    "lodash": "^4.17.4",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react-loadable": "^5.4.0",
    "react-router-dom": "^4.3.1",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3"
  },
  "devDependencies": {
    "better-npm-run": "^0.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "less": "^3.0.4",
    "less-loader": "^4.1.0"
  },
  "license": "MIT",
  "private": true
}